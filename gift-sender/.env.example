# Example environment file - copy to .env and fill with actual values
# This uses the same environment variables as the main project

# Telegram Bot Token (same as main app)
TG_BOT_TOKEN=your_telegram_bot_token_here

# Database configuration (adjust host for Docker network)
POSTGRES_HOST=postgres  # Use service name when running in Docker network
POSTGRES_PORT=5432
POSTGRES_DB=crash_stars
POSTGRES_USER=username
POSTGRES_PASSWORD=password

# Or use full DATABASE_URL
# DATABASE_URL=postgresql+asyncpg://username:password@postgres:5432/crash_stars