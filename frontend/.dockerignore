# Exclude build artifacts and compiled files
dist/
build/
.next/
out/

# Exclude compiled JavaScript files from TypeScript sources
src/**/*.js
src/**/*.js.map
!src/**/*.config.js

# Exclude development files
.env.local
.env.development.local
.env.test.local
.env.production.local

# Exclude cache and temporary files
.cache/
.tmp/
.temp/
*.tmp
*.temp

# Exclude IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# Exclude OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Exclude logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Exclude dependency directories
node_modules/

# Exclude coverage directory used by tools like istanbul
coverage/
*.lcov

# Exclude test files (optional)
**/*.test.ts
**/*.test.tsx
**/*.spec.ts
**/*.spec.tsx

# Exclude documentation
README.md
docs/
*.md

# Exclude git files
.git/
.gitignore